{
  "name": "@cookbook/ui",
  "version": "0.0.1",
  "description": "Cookbook frontend UI",
  "main": "dist/app.js",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@cookbook/shared": "^0.0.1"
  },
  "devDependencies": {
    "typescript": "^4.5.2"
  },
  "scripts": {
    "build": "tsc --build",
    "clean": "rm -rf ./dist && rm ./tsconfig.tsbuildinfo",
    "build:clean": "yarn clean && yarn build",
    "prestart": "yarn build",
    "start": "node .",
    "start:watch": "tsc-watch --build --noClear --onSuccess 'node .'"
  }
}
